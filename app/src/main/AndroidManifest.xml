<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.thetechnocafe.tutorials">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScrollingFABActivity" />
        <activity android:name=".OkHttpActivity" />
        <activity android:name=".RecyclerViewPagerActivity" />
        <activity android:name=".ConstraintLayout" />
        <activity android:name=".PaintingWIthViewsActivity" />
        <activity android:name=".MovingBackgroundGradientActivity" />
        <activity android:name=".HighlightTextActivity" />
        <activity android:name=".AnimatorActivity" />
        <activity android:name=".DrawableActivity" />
        <activity android:name=".RetroLambdaActivity"></activity>
    </application>

</manifest>